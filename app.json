{
  "expo": {
    "name": "BuzzWheel",
    "slug": "BuzzWheel",
    "version": "1.0.2",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/images/icons_ios/icon-e.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "locales": {
      "en": "./assets/locales/en.json",
      "ru": "./assets/locales/ru.json"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibragimlol.BuzzWheel",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons_android/icon-transparent.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["com.google.android.gms.permission.AD_ID"],
      "package": "com.ibragimlol.BuzzWheel"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0C111C",
          "android": {
            "image": "./assets/images/icons_android/icon-transparent.png",
            "resizeMode": "contain"
          }
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Buzz Wheel to use the microphone to record your voice for the selected functionality."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "buzzwheel",
          "organization": "mydream-6o"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f969f83-2d04-4a6a-ab0a-44323df0de8c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "ibragimlol",
    "updates": {
      "url": "https://u.expo.dev/1f969f83-2d04-4a6a-ab0a-44323df0de8c"
    }
  }
}
