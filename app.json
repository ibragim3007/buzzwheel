{
  "expo": {
    "name": "BuzzWheel",
    "slug": "BuzzWheel",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/images/icons_ios/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibragimlol.BuzzWheel",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons_android/icon-filled.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ibragimlol.BuzzWheel"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0C111C",
          "android": {
            "image": "./assets/images/icons_android/icon-transparent.png",
            "resizeMode": "contain"
          }
        }
      ],
      [
        "expo-alternate-app-icons",
        [
          {
            "name": "IconA",
            "ios": "./assets/images/icons_ios/icon-a.png",
            "android": {
              "backgroundColor": "#001413",
              "foregroundImage": "./assets/images/icons_ios/icon-a.png"
            }
          },
          {
            "name": "IconB",
            "ios": "./assets/images/icons_ios/icon-b.png",
            "android": {
              "backgroundColor": "#001413",
              "foregroundImage": "./assets/images/icons_ios/icon-b.png"
            }
          },
          {
            "name": "IconC",
            "ios": "./assets/images/icons_ios/icon-c.png",
            "android": {
              "backgroundColor": "#001413",
              "foregroundImage": "./assets/images/icons_ios/icon-c.png"
            }
          },
          {
            "name": "IconD",
            "ios": "./assets/images/icons_ios/icon-d.png",
            "android": {
              "backgroundColor": "#001413",
              "foregroundImage": "./assets/images/icons_ios/icon-d.png"
            }
          }
        ]
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow BuzzWheel to access your microphone."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "buzzwheel",
          "organization": "mydream-6o"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f969f83-2d04-4a6a-ab0a-44323df0de8c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "ibragimlol",
    "updates": {
      "url": "https://u.expo.dev/1f969f83-2d04-4a6a-ab0a-44323df0de8c"
    }
  }
}
